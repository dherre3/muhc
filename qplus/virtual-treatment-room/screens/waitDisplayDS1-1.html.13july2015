<body>
<div ng-controller="waitController">
<center>

<table border=0 style="width: 100%">
<tr>
  <td align="right" valign="center">
    <!--<h2><b>Gestion de la salle d'attente</b></h2> -->
  </td>
  <td rowspan="1" align="center" valign="bottom">
    <center>
    <img src="http://172.26.66.41/Documents/config/images/CedarsLogo.jpg" width=400px></img>
    </center>
  </td>
  <td align="left" valign="center">
    <!--<h2><b>Waiting Room Management</b></h2> -->
  </td>
</tr>
</table>

<p/>
  <table class="table table-hover" border="0" style="width: 1900px;">
    <tr class="bg-success">
    <th style="width: 600px;"><h1><strong>Patient</strong></h1></th>
    <th style="width: 1300px;"><h1><strong>Destination</strong></h1></th>
    </tr>
    <tr ng-repeat="item in screenRows | orderBy: '-Timestamp' | filter:{'PatientStatus': 'Called'}" ng-class=" (item.diff > 0) && (item.diff < 18000) ? 'bg-danger': '' ">
      <td>{{ item.FirstName}} {{ item.LastName}}<span class="invisible">{{ item.diff = timeNow - item.Timestamp}}</span></td>  
      <td>{{item.Destination}}</td>  
    </tr>
  </table>
<p/>

</div>
</center>
